{
   "compilerOptions": {
      "declaration": true,
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "lib": ["ES6", "DOM"],
      "module": "ES6",
      "moduleResolution": "Node",
      "noFallthroughCasesInSwitch": true,
      "noImplicitReturns": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "outDir": "./lib",
      "skipLibCheck": true,
      "strict": true,
      "target": "ES6"
   }
}
